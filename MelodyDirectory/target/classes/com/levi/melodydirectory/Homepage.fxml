<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>


<VBox fx:id="rootVBox" prefHeight="420.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.levi.melodydirectory.HomePageController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label text="Top Rated">
               <VBox.margin>
                  <Insets left="1.0" />
               </VBox.margin>
            </Label>
            <ListView fx:id="topRatedSlider" orientation="HORIZONTAL" prefHeight="200.0" prefWidth="200.0" />
         </children>
      </VBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label text="Recently Added">
               <VBox.margin>
                  <Insets left="1.0" />
               </VBox.margin>
            </Label>
            <ListView fx:id="recentlyAddedSlider" orientation="HORIZONTAL" prefHeight="200.0" prefWidth="200.0" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
